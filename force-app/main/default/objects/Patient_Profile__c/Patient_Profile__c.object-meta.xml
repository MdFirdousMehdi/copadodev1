<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <allowInChatterGroups>false</allowInChatterGroups>
    <deploymentStatus>Deployed</deploymentStatus>
    <enableActivities>true</enableActivities>
    <enableHistory>true</enableHistory>
    <enableReports>true</enableReports>
    <label>Patient Profile</label>
    <pluralLabel>Patient Profiles</pluralLabel>
    <sharingModel>ReadWrite</sharingModel>
    <nameField>
        <label>Patient Profile Name</label>
        <type>Text</type>
    </nameField>

    <!-- Age -->
    <fields>
        <fullName>Age__c</fullName>
        <externalId>false</externalId>
        <label>Age</label>
        <precision>3</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
    </fields>

    <!-- Gender -->
    <fields>
        <fullName>Gender__c</fullName>
        <externalId>false</externalId>
        <label>Gender</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Male</fullName>
                    <default>false</default>
                    <label>Male</label>
                </value>
                <value>
                    <fullName>Female</fullName>
                    <default>false</default>
                    <label>Female</label>
                </value>
                <value>
                    <fullName>Other</fullName>
                    <default>false</default>
                    <label>Other</label>
                </value>
                <value>
                    <fullName>Unknown</fullName>
                    <default>true</default>
                    <label>Unknown</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>

    <!-- Condition -->
    <fields>
        <fullName>Condition__c</fullName>
        <externalId>false</externalId>
        <label>Condition</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
    </fields>

    <!-- Last Visit Date -->
    <fields>
        <fullName>Last_Visit_Date__c</fullName>
        <externalId>false</externalId>
        <label>Last Visit Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>

    <!-- Risk Score (0-10 with 1 decimal) -->
    <fields>
        <fullName>Risk_Score__c</fullName>
        <externalId>false</externalId>
        <label>Risk Score</label>
        <precision>3</precision>
        <required>false</required>
        <scale>1</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
    </fields>

    <!-- Preferred Contact Channel -->
    <fields>
        <fullName>Contact_Channel__c</fullName>
        <externalId>false</externalId>
        <label>Contact Channel</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>SMS</fullName>
                    <default>false</default>
                    <label>SMS</label>
                </value>
                <value>
                    <fullName>Email</fullName>
                    <default>true</default>
                    <label>Email</label>
                </value>
                <value>
                    <fullName>Phone</fullName>
                    <default>false</default>
                    <label>Phone</label>
                </value>
                <value>
                    <fullName>Portal</fullName>
                    <default>false</default>
                    <label>Portal</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>

    <!-- Outreach Consent -->
    <fields>
        <fullName>Consent__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <label>Consent for Outreach</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>

</CustomObject>
